year <- 2017
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{vein}: Vehicular Emissions Inventories",
         author = c(person("Sergio", "Ibarra-Espinosa")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=vein")
